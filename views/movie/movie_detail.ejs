<h1><%=movie.title%></h1>
<p>Released: <%= movie.release_date %></p>
<p>Summary: <%= movie.summary %></p>
<img src="<%= movie.poster %>" alt="<%= movie.title %>" />
<h3>Cast:</h3>
<% if (cast.length > 0) { %> <% cast.forEach(actor => { %>
<p><%= actor.name %></p>
<% }); %> <% } else { %>
<p>No cast to display</p>
<% } %>
