<div class="list-page">
  <h1>List of Genres</h1>
  <% if (genres.length) { %>
  <ul class="item-list">
    <% genres.forEach(genre => { %>
    <li class="genre-info">
      <a href="/genres/<%= genre._id %>"><h3><%= genre.name %></h3></a>
    </li>
    <% }); %>
  </ul>
  <% } else { %>
  <p>There are no kinds of genre registered</p>
  <% } %>
</div>
